#!/bin/bash
EXCLUDE=(
  "--exclude tmdb-data"
  "--exclude tmdb-docs"
  "--exclude tmdb-elasticsearch"
  "--exclude tmdb-emailer"
  "--exclude tmdb-keywords"
  "--exclude tmdb-payments"
  "--exclude tmdb-web-translations"
)

fd tmdb- --full-path --maxdepth=1 --type=d --color=never ${EXCLUDE[@]} $HOME/workspace
