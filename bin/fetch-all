#! /bin/bash

set -e

PROJECTS=(
  platform-apis
  data-acquisition
  crawlers
  PetroFeedApp
  PetroFeedApp-Android
  petrofeed-connect-server
  petrofeed-connect-client)
for project in "${PROJECTS[@]}"; do
  echo "Fetching $project"

  project_path="${HOME}/workspace/$project"
  cd $project_path

  git fetch origin
done
