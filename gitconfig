[alias]
  aa = add --all
  amend = commit --amend
  au = add --update
  b = branch
  c = commit
  co = checkout
  d = diff -M
  dc = diff --cached
  ds = diff --stat
  diffc = diff --cached
  discard = checkout --patch
  deploy-msg = "!source ~/.githelpers && deploy_git_log"
  ff = "!source ~/.githelpers && ffwd"
  ffx = merge --ff-only
  h = !git head
  head = !git l -1
  hp = "!source ~/.githelpers && show_git_head"
  l = "!source ~/.githelpers && pretty_git_log"
  la = !git l --all
  ld = !git l --simplify-by-decoration
  lt = !git l --since=yesterday
  noff = merge --no-ff
  p = add --patch
  po = push origin
  pod = push origin develop
  pom = push origin master
  phm = push heroku master
  r = !git --no-pager l -20
  ra = !git r --all
  s = status -sb
  st = status
  unstage = reset --patch
