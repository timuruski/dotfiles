[github]
  user = timuruski
  token = !security 2>&1 >/dev/null find-generic-password -gs github.token | ruby -e 'print $1 if STDIN.gets =~ /^password: \\\"(.*)\\\"$/'
  password = !security 2>&1 >/dev/null find-generic-password -gs github.password | ruby -e 'print $1 if STDIN.gets =~ /^password: \\\"(.*)\\\"$/'
[color]
  ui = auto
[color "diff"]
  whitespace = red reverse
[alias]
  aa = add --all
  au = add --update
  b = branch --sort='committerdate'
  c = commit
  ci = "!source ~/.githelpers && fuzzy_checkout"
  conflicts = diff --name-only --diff-filter=U
  d = diff -M
  dc = diff --cached
  ds = diff --stat
  diffc = diff --cached
  discard = checkout --patch
  deploy-msg = "!source ~/.githelpers && deploy_git_log"
  ff = "!source ~/.githelpers && ffwd"
  ffx = merge --ff-only
  fix = commit --fixup head
  git = !git
  h = !git head
  head = !git l -1
  hp = "!source ~/.githelpers && show_git_head"
  l = "!source ~/.githelpers && pretty_git_log"
  la = !git l --all
  ld = !git l --simplify-by-decoration
  local-ignore = ignore-local
  lt = !git l --since=yesterday
  new-branch = switch --create
  oldest-ancestor = !zsh -c 'diff --old-line-format= --new-line-format= <(git rev-list --first-parent \"${1:-master}\") <(git rev-list --first-parent \"${2:-HEAD}\") | head -1' -
  p = add --patch
  po = push origin
  pob = !source ~/.githelpers && push_origin
  pom = push origin master
  phm = push heroku master
  r = !git l -20
  ; r = !git --no-pager log --decorate --oneline --graph --max-count=20
  ra = !git r --all
  s = status -sb
  ss = status -sb
  sw = fuzzy-switch
  show-upstream = "rev-parse --abbrev-ref --symbolic-full-name @{u}"
  update = pull --ff-only
  unstage = reset --patch
  z = select
[core]
  excludesfile = /Users/timuruski/.gitignore_global
	commitGraph = true
[diff]
[push]
	default = simple
[pull]
	ff = only
[credential]
	helper = osxkeychain
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[gc]
	writeCommitGraph = true
[init]
	defaultBranch = main
